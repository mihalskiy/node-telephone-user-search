{
  "name": "node-telephone-user-search",
  "version": "1.0.0",
  "description": "node-telephone-user-search \n If you dont have install node-js, please go this link [https://nodejs.org](https://nodejs.org)",
  "main": "index.js",
  "config": {
    "engine": "getcontact",
    "dockerHostUrl": "197229461665.dkr.ecr.us-east-2.amazonaws.com"
  },
  "scripts": {
    "//prestart": "npm run validateSwagger && npm run fix",
    "//db:create": "node_modules/.bin/create_database",
    "//db:drop": "node_modules/.bin/drop_database",
    "start": "nodemon ./bin/www -w app.js -w server  -e js,json,yaml",
    "validateSwagger": "swagger-tools validate ./server/middleware/swagger/swaggerDocs.yaml",
    "fix": "eslint --fix .",
    "db:init": "sequelize db:migrate",
    "db:createMigration": "sequelize migration:create --name migration",
    "dockerLocal": "npm run predocker && docker run -e $npm_package_config_engine .",
    "predocker": "docker build -t $npm_package_config_engine .",
    "docker": "docker tag $npm_package_config_engine $npm_package_config_dockerHostUrl/$npm_package_config_engine:$npm_package_version",
    "postdocker": "docker push $npm_package_config_dockerHostUrl/$npm_package_config_engine:$npm_package_version"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/helmet": "0.0.37",
    "@types/morgan": "^1.7.35",
    "@types/sequelize": "^4.27.8",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.15.2",
    "cookie-parser": "^1.4.3",
    "express": "^4.14.0",
    "express-session": "^1.15.6",
    "js-yaml": "^3.11.0",
    "jsonwebtoken": "^8.2.0",
    "morgan": "^1.9.0",
    "pg": "6.1.0",
    "pg-hstore": "^2.3.2",
    "phone": "^2.0.0-rc.1",
    "sequelize": "^4.0.0",
    "sequelize-migration-pg-extras": "^1.0.1",
    "swagger-express-middleware": "^1.0.0-alpha.12",
    "swagger-ui-express": "^2.0.15"
  },
  "devDependencies": {
    "eslint": "^4.18.2",
    "eslint-config-google": "^0.9.1",
    "nodemon": "^1.11.0",
    "pgtools": "^0.3.0",
    "swagger-tools": "^0.10.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mihalskiy/node-telephone-user-search.git"
  },
  "bugs": {
    "url": "https://github.com/mihalskiy/node-telephone-user-search/issues"
  },
  "homepage": "https://github.com/mihalskiy/node-telephone-user-search#readme"
}
